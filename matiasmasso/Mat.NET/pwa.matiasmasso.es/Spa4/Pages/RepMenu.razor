@page "/repMenu/{Guid:guid}"
@layout ProLayout

<Nav nav="@model"></Nav>
@code {
    [Parameter] public Guid Guid { get; set; }
    private NavDTO? model;

    protected override void OnParametersSet()
    {
        base.OnParametersSet();
        if (model == null)
        {
            model = new NavDTO();
            model.AddItem("/rep/" + Guid.ToString(), "Ficha", "Fitxa", "Details");
            model.AddItem("/repliqs/" + Guid.ToString(), "Liquidaciones", "Liquidacions", "Statements");
            model.AddItem("/certificatsIrpf/" + Guid.ToString(), "Certificados Irpf", "Certificats Irpf", "Tax certificates");
            StateHasChanged();
        }
    }
    }


